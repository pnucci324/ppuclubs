<div style="height:25px"></div>
<h1 class="text-center">Group Search</h1>
<div class="container">
      <form>
        <div class="form-group col-xs-2 ">
          <label>Group Name</label>
          <input type="text" class="form-control" placeholder="Insert Group Name">
          <br>
          <button type="submit" class="btn btn-default">Search</button>
        </div>

      <table class="table">
        <thead>
          <th scope="col"> Group Name </th>
          <th score="col"> Group Description </th>
        </thead>
      <tbody>
        {{#each results}}
        <tr>
            <td>{{this.GroupName}}</td>
            <td>{{this.GroupDescription}}</td>
            <td><button type="submit" class="btn btn-info">Join</button>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </form>
</div>
