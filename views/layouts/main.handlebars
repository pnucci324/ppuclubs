    <script>
        $(function() {
          $(".deleteUser").click( function(e)
          {
            console.log($(this).attr("id"));
            var parts = $(this).attr("id").split("_");
            //pointgroups.removeUser();
            $.ajax({
              type: "POST",
              url: "deleteUser",
              data: {
                userID: parts[1],
                groupID: parts[2]
              },
              success: function(data){
                console.log(data);
                location.reload();
              },
              error: function(a, b, c){
                alert(a + b + c);
                console.log(a);
                console.log(b);
                console.log(c);
              }

            })
          }
        );
      });
    </script>

